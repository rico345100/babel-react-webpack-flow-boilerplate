{
	"name": "babel-react-webpack-flow-boilerplate",
	"version": "1.0.0",
	"description": "Boilerplate for Babel + React + Flow + Webpack Combo",
	"main": "index.js",
	"scripts": {
		"build": "webpack --mode development --progress",
		"build-analyze": "BUILD_ANALYZE=1 webpack --mode production --progress --config webpack.prod.js --profile --json > prod-result.json",
		"build-prod": "webpack --mode production --progress --config webpack.prod.js",
		"dev": "webpack-dev-server --mode development --open --progress --inline",
		"flow": "flow",
		"start": "npm run build-prod && npm run serve",
		"serve": "node server"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rico345100/babel-react-webpack-flow-boilerplate"
	},
	"keywords": [
		"babel",
		"react",
		"webpack",
		"flow",
		"boilerplate"
	],
	"author": ".modernator",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-proposal-decorators": "^7.0.0",
		"@babel/plugin-transform-flow-strip-types": "^7.0.0",
		"@babel/plugin-transform-react-jsx-source": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@hot-loader/react-dom": "^16.8.6",
		"babel-loader": "^8.0.0",
		"clean-webpack-plugin": "^0.1.19",
		"core-js": "^3.0.1",
		"css-loader": "^2.1.1",
		"css-module-flow": "^1.0.0",
		"file-loader": "^3.0.1",
		"flow-bin": "^0.69.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.1.0",
		"mini-css-extract-plugin": "^0.4.0",
		"node-sass": "^4.12.0",
		"prop-types": "^15.6.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-hot-loader": "^4.8.4",
		"react-redux": "^5.0.7",
		"redux": "^3.7.2",
		"sass-loader": "^6.0.7",
		"style-loader": "^0.20.3",
		"url-loader": "^1.1.2",
		"webpack": "^4.4.1",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "^3.1.1",
		"webpack-dev-server": "^3.1.11"
	},
	"dependencies": {
		"express": "^4.16.3",
		"express-history-api-fallback": "^2.2.1",
		"serve-favicon": "^2.5.0"
	}
}
